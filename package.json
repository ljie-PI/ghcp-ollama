{
  "name": "@ljie-pi/ghcp-ollama",
  "version": "0.1.6",
  "description": "Using LLMs provided by Github Copilot with Ollama APIs",
  "main": "src/server.js",
  "keywords": [
    "github-copilot",
    "ollama",
    "openai",
    "anthropic",
    "llm",
    "ai",
    "chatbot",
    "api-proxy"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ljie-PI/ghcp-ollama.git"
  },
  "bugs": {
    "url": "https://github.com/ljie-PI/ghcp-ollama/issues"
  },
  "homepage": "https://github.com/ljie-PI/ghcp-ollama#readme",
  "author": "ljie-PI",
  "license": "MIT",
  "files": [
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "node build.js",
    "server:start": "node src/serverctl.js start",
    "server:stop": "node src/serverctl.js stop",
    "server:restart": "node src/serverctl.js restart",
    "server:status": "node src/serverctl.js status",
    "test": "vitest",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:golden": "node tests/integration/utils/golden_generator.js"
  },
  "bin": {
    "ghcpo": "./src/ghcpo.js",
    "ghcpo-server": "./src/serverctl.js"
  },
  "dependencies": {
    "eslint_d": "^14.3.0",
    "express": "^4.18.2",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@types/express": "^4.17.21",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.14.10",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "eslint": "^9.35.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^15.9.0",
    "nodemon": "^3.1.9",
    "ollama": "^0.6.3",
    "openai": "^6.16.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module"
}
